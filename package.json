{
  "name": "tgas-local",
  "version": "1.0.5",
  "description": "Typescript support for running and testing GAS projects locally.",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "clean": "rimraf --glob ./lib",
    "build": "pnpm run clean && tsc -p ./tsconfig.json",
    "prepack": "pnpm run build"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "Google Apps Script",
    "gas",
    "typescript",
    "gas-local",
    "googleappsscript"
  ],
  "author": "Seth Aker <saker132@gmail.com> (https://github.com/seth-aker)",
  "repository": {
    "url": "https://github.com/seth-aker/tgas-local"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "esbuild": "^0.25.11",
    "glob": "^11.0.3"
  },
  "peerDependencies": {
    "@types/google-apps-script": "^2.0.7"
  },
  "devDependencies": {
    "@types/google-apps-script": "^2.0.7",
    "@types/node": "^24.9.2",
    "rimraf": "^6.1.0",
    "type-fest": "^5.2.0",
    "typescript": "^5.9.3"
  }
}
